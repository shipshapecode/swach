  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "release-it": {
    "plugins": {
      "@release-it/bumper": {
        "out": ["electron-app/package.json"]
      },
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": false,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": false
  }
