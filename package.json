{
  "name": "swach",
  "version": "0.1.44",
  "private": true,
  "description": "A robust color management tool for the modern age.",
  "repository": "https://github.com/shipshapecode/swach",
  "license": "MIT",
  "author": {
    "name": "Robert Wagner",
    "email": "rwwagner90@gmail.com",
