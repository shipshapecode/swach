{
  "name": "swach",
  "productName": "swach",
  "version": "0.2.7",
  "description": "A robust color management tool for the modern age.",
  "main": "src/index.js",
  "scripts": {
    "build": "electron-builder",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "postinstall": "electron-builder install-app-deps",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Robert Wagner",
    "email": "rwwagner90@gmail.com",
    "url": "https://github.com/rwwagner90"
  },
  "license": "MIT",
  "config": {
    "forge": "config.forge.js"
  },
  "dependencies": {
    "@sentry/electron": "^2.0.0",
    "auto-launch": "^5.0.5",
    "electron-color-picker": "^0.2.0",
    "electron-dl": "^3.0.1",
    "electron-is-dev": "^1.2.0",
    "electron-notification-state": "^1.0.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.0",
    "indexeddb-export-import": "^2.1.1",
    "menubar": "^9.0.1",
    "throttle-debounce": "^2.3.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-deb": "^6.0.0-beta.53",
    "@electron-forge/maker-dmg": "^6.0.0-beta.53",
    "@electron-forge/maker-snap": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "@sentry/cli": "^1.56.1",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.0",
    "electron-debug": "^3.1.0",
    "electron-download": "^4.1.1"
  },
  "engines": {
    "node": ">= 12"
  },
  "build": {
    "productName": "Swach",
    "linux": {
      "icon": "./resources/icon.png",
      "category": "Development",
      "description": "A robust color management tool for the modern age.",
      "synopsis": "A robust color management tool for the modern age.",
      "target": [
        "snap"
      ]
    },
    "snap": {
      "stagePackages": [
        "default",
        "giblib1",
        "libimlib2",
        "libx11-6",
        "libxcursor1",
        "libxfixes3",
        "scrot"
      ]
    }
  }
