    "vite-plugin-testem-electron": "^1.0.2",
    "vitest": "^4.0.10",
    "wcag-contrast": "^3.0.0"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">= 22",
    "pnpm": "^10.20.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "pnpm": {
    "overrides": {
      "@ember/string": "^4.0.1"
    },
    "patchedDependencies": {
      "ember-local-storage": "patches/ember-local-storage.patch",
      "object-inspect": "patches/object-inspect.patch"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "core-js",
      "electron",
      "electron-winstaller",
      "esbuild",
      "fs-xattr",
      "fsevents",
      "macos-alias"
    ]
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": false,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": {
      "publish": false
    }
  }
}
